<Window x:Class="WpfAppTarovPr2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="445" Width="667" Icon="/Resources/i.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="60" />
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="#FF76E383">
            <Button Name="btnBack" HorizontalAlignment="Right" Content="Назад" Click="btnBack_Click"/>
            <Image Margin="10,10,475,12" Width="40" Height="40" 
          HorizontalAlignment="Left" Source="/Resources/Image1.png" 
          Stretch="Uniform" Grid.Row="0"/>
        </Grid>
        <Frame x:Name="FrmMain" Grid.Row="1" NavigationUIVisibility="Hidden" ContentRendered="FrmMain_ContentRendered"/>
    </Grid>
</Window>






